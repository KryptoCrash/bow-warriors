{
  "rulesDirectory": [
    "./"
  ],
  "rules": {
    "indent": [
      true,
      "tabs",
      2
    ],
    "max-line-length": {
      "options": [
        180
      ]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": {
      "options": [
        "debug",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    }
  },
  "jsRules": {
    "max-line-length": {
      "options": [
        120
      ]
    }
  }
}